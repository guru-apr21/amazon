{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle":"off",
        "consistent-return":"off",
        "comma-dangle":"off",
        "object-curly-newline":["error",{
            "ObjectPattern":"never",
            "ImportDeclaration": "off"
        }],
        "no-param-reassign":["error",{"props":false}]
    }
}
